    case " $hid_exporters " in
      *\ $e\ * )
         HIDLIST="$HIDLIST $e"
	 ;;
      * ) { { echo "$as_me:$LINENO: error: $e is not a valid exporter" >&5
echo "$as_me: error: $e is not a valid exporter" >&2;}
   { (exit 1); exit 1; }; } ;;
    esac
done

