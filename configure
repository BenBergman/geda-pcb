     test "x$exec_prefix" = xNONE && exec_prefix="${prefix}"
     _lcl_receval_old=''
     while test "$_lcl_receval_old" != "$_lcl_receval"; do
       _lcl_receval_old="$_lcl_receval"
       eval _lcl_receval="\"$_lcl_receval\""
